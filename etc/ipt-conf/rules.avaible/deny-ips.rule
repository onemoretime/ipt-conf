. $CONFDIR/rules.enabled/deny-ips.conf

# SUBNETS $IPS
# SUBNETS $IPSV6
echo

for IP in $IPS ; do 
	echo -n "Blocking $IP "
	$iptables -A INPUT -p all -s $IP -j DROP && echo "[OK]" || echo "[ERROR]"
done

if [ "$IPV6_ENABLE" = "YES" ] ; then 

	for IP in $IPSV6 ; do 
		echo -n "Blocking $IP "
		$ip6tables -A INPUT -p all -s $IP -j DROP && echo "[OK]" || echo "[ERROR]"
	done
fi
